{
  "root": true,
  "env": {
    "node": true,
    "es2022": true
  },
  "extends": [
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": [
      "warn",
      {
        "args": "none"
      }
    ],
    "no-console": "off"
  },
  "ignorePatterns": [
    "uploads/**",
    "public/**",
    "tests/**"
  ]
}